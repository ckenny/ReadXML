									}
								},
								{
									"section": {
										"id": {
											"root": "891bf52f-e3ae-4128-a417-a398d9a50c0c"
										},
										"code": {
											"code": "50565-1",
											"codeSystem": "2.16.840.1.113883.6.1",
											"displayName": "OTC - KEEP OUT OF REACH OF CHILDREN SECTION"
										},
										"title": "Keep out of reach of children.",
										"text": {
											"paragraph": {
												"content": {
													"styleCode": "bold",
													"text": ")."
												},
												"text": "In case of overdose, get medical help or contact a Poison Control Center right away (1-800-222-1222"
											}
										},
										"effectiveTime": {
											"value": "20180803"
										},
										"ID": "i4i_OTC_Keep_Away_Children_id_2201691b-fe61-4087-87dc-4bfbe962d134"
									}
								}
							],
							"ID": "i4i_warnings_id_59d9d4f9-b37e-49ba-a211-777bb2a06925"
						}
					},
					{
						"section": {
							"id": {
								"root": "c2545201-df2b-4c74-9d73-00163508e221"
							},
							"code": {
								"code": "34068-7",
								"codeSystem": "2.16.840.1.113883.6.1",
								"displayName": "DOSAGE & ADMINISTRATION SECTION"
							},
							"title": "Directions",
							"text": {
								"list": {
									"item": [
										{
											"caption": "•",
											"text": "adults and children 12 years and over:"
										},
										{
											"caption": "•",
											"text": "to relieve symptoms, swallow 1 tablet with a glass of water"
										},
										{
											"caption": "•",
											"text": "to prevent symptoms, swallow 1 tablet with a glass of water 30 to 60 minutes before eating food or drinking beverages that cause heartburn"
										},
										{
											"caption": "•",
											"text": "can be used up to twice daily (do not take more than 2 tablets in 24 hours)"
										},
										{
											"caption": "•",
											"text": "children under 12 years: ask a doctor"
										}
									],
									"listType": "unordered"
								}
							},
							"effectiveTime": {
								"value": "20180803"
							},
							"ID": "i4i_dosage_admin_id_d7ac69db-9be2-419c-8a68-e5b66c68b4ff"
						}
					},
					{
