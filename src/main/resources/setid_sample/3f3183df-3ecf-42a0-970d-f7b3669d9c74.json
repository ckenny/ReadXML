							"code": {
								"code": "55105-1",
								"codeSystem": "2.16.840.1.113883.6.1",
								"displayName": "OTC - PURPOSE SECTION"
							},
							"title": "Purpose",
							"text": {
								"paragraph": "Acid reducer"
							},
							"effectiveTime": {
								"value": "20180803"
							},
							"ID": "i4i_OTC_Purpose_id_0d423011-b34e-4ea2-9912-1c083cb52e67"
						}
					},
					{
						"section": {
							"id": {
								"root": "f47d77b2-e137-42cc-ad1e-9ecb2b838a53"
							},
							"code": {
								"code": "34067-9",
								"codeSystem": "2.16.840.1.113883.6.1",
								"displayName": "INDICATIONS & USAGE SECTION"
							},
							"title": "Uses",
							"text": {
								"list": {
									"item": [
										{
											"caption": "•",
											"text": "relieves heartburn associated with acid indigestion and sour stomach"
										},
										{
											"caption": "•",
											"text": "prevents heartburn associated with acid indigestion and sour stomach brought on by eating or drinking certain foods and beverages"
										}
									],
									"listType": "unordered"
								}
							},
							"effectiveTime": {
								"value": "20180803"
							},
							"ID": "i4i_indications_id_4f72e7db-541a-4b6c-bc3a-3dff7064c84e"
						}
					},
					{
						"section": {
							"id": {
								"root": "cf41f35d-9f4c-4a7b-a164-40673c353492"
							},
							"code": {
								"code": "34071-1",
								"codeSystem": "2.16.840.1.113883.6.1",
								"displayName": "WARNINGS SECTION"
							},
							"title": "Warnings",
							"text": {
								"paragraph": "Allergy alert: Do not use if you are allergic to ranitidine or other acid reducers"
							},
							"effectiveTime": {
								"value": "20180803"
							},
							"component": [
								{
									"section": {
										"id": {
											"root": "ab912129-4aa2-4bf4-9283-e36afd779e64"
										},
										"code": {
											"code": "50570-1",
											"codeSystem": "2.16.840.1.113883.6.1",
											"displayName": "OTC - DO NOT USE SECTION"
										},
										"title": "Do not use",
										"text": {
											"list": {
												"item": [
													{
														"caption": "•",
														"text": "if you have trouble or pain swallowing food, vomiting with blood, or bloody or black stools. These may be signs of a serious condition. See your doctor."
													},
													{
														"caption": "•",
														"text": "with other acid reducers"
													},
													{
														"caption": "•",
														"text": "if you have kidney disease, except under the advice and supervision of a doctor"
													}
												],
												"listType": "unordered"
											}
										},
										"effectiveTime": {
											"value": "20180803"
										},
										"ID": "i4i_OTC_Do_not_use_id_fa02cf9f-5e85-4da8-bcb8-29e5d6a17563"
									}
								},
								{
									"section": {
										"id": {
											"root": "ea480ea7-0518-4459-b036-3e5329eb8707"
										},
										"code": {
											"code": "50569-3",
											"codeSystem": "2.16.840.1.113883.6.1",
											"displayName": "OTC - ASK DOCTOR SECTION"
										},
										"title": "Ask a doctor before use if you have",
										"text": {
											"list": {
												"item": [
													{
														"caption": "•",
														"text": "had heartburn over 3 months. This may be a sign of a more serious condition."
													},
													{
														"caption": "•",
														"text": "heartburn with lightheadedness, sweating or dizziness"
													},
													{
														"caption": "•",
														"text": "chest pain or shoulder pain with shortness of breath; sweating; pain spreading to arms, neck or shoulders; or lightheadedness"
													},
													{
														"caption": "•",
														"text": "frequent chest pain"
													},
													{
														"caption": "•",
														"text": "frequent wheezing, particularly with heartburn"
													},
													{
														"caption": "•",
														"text": "unexplained weight loss"
													},
													{
														"caption": "•",
														"text": "nausea or vomiting"
													},
													{
														"caption": "•",
														"text": "stomach pain"
													}
												],
												"listType": "unordered"
											}
										},
										"effectiveTime": {
											"value": "20180803"
										},
										"ID": "i4i_OTC_Ask_Doctor_id_6665c04c-d4ac-4f00-b4fd-e8c13d7a96c1"
									}
								},
								{
									"section": {
										"id": {
											"root": "735dc0aa-d763-4fe5-b9bf-47733f11c48f"
										},
										"code": {
											"code": "50566-9",
											"codeSystem": "2.16.840.1.113883.6.1",
											"displayName": "OTC - STOP USE SECTION"
										},
										"title": "Stop use and ask a doctor if",
										"text": {
											"list": {
												"item": [
													{
														"caption": "•",
														"text": "your heartburn continues or worsens"
													},
													{
														"caption": "•",
														"text": "you need to take this product for more than 14 days"
													}
												],
												"listType": "unordered"
											}
										},
										"effectiveTime": {
											"value": "20180803"
										},
										"ID": "i4i_OTC_Stop_Use_id_4e617852-843d-44d3-85b5-58bca961a31d"
									}
								},
